<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.upc.semana5">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Semana5"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".AvisosActivity"
            android:label="@string/title_activity_avisos"></activity>
        <activity android:name=".AvisosRegistrarActivity"></activity>
        <activity android:name=".AvisosBuscarActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProductoRegistrarActivity"></activity>
        <activity android:name=".ProductosBuscarActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>